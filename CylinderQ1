class Cylinder {
    private double radius;
    private double height;

    // Setters
    public void setRadius(double r) {
        radius = r;
    }

    public void setHeight(double h) {
        height = h;
    }

    // Getters
    public double getRadius() {
        return radius;
    }

    public double getHeight() {
        return height;
    }
}
